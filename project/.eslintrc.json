{
  "extends": ["eslint:recommended", "airbnb-base"],
  "plugins": ["import", "babel"],
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "ignorePatterns": ["node_modules", "dist"],
  "rules": {
    "no-console": 0
  }
}
